    "webpack": "4.47.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "4.15.1",
    "webpack-manifest-plugin": "3.1.1",
    "webpack-merge": "4.2.2",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>setup/enzyme.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
