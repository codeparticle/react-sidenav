    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "node-sass-chokidar": "^2.0.0",
    "node-uuid": "^1.4.8",
    "npm-run-all": "^4.1.5",
    "object-assign": "4.1.1",
    "promise": "8.3.0",
    "prop-types": "^15.6.2",
    "raf": "3.4.1",
    "react": "^16.4.1",
    "react-dev-utils": "^12.0.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hot-loader": "^4.13.0",
