{
  "name": "@codeparticle/react-sidenav",
  "version": "24.0.0",
  "description": "A generic sidenav component for React",
  "main": "index.js",
  "scripts": {
    "demo": "npm-run-all -p watch-css start-js",
    "publish-preview": "preview",
    "test": "jest ./test",
    "lint": "eslint src/** --fix",
    "build-css": "rm -rf ./public/css && ./node_modules/.bin/node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o ./public/css --output-style expanded",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o ./public/css --watch --recursive --output-style expanded",
    "start-js": "node scripts/start.js",
    "release:major": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "prepublish": "NODE_ENV=production ./node_modules/.bin/webpack --config ./config/webpack.config.publish.js && cp ./.npmrc ./build/.npmrc && cp ./CHANGELOG.md ./build/CHANGELOG.md && cp ./README.md ./build/README.md",
    "publish:npm": "npm run prepublish && npm publish ./build --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeparticle/react-sidenav.git"
  },
  "keywords": [
    "react",
    "list",
    "codeparticle"
  ],
  "author": "Code Particle",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codeparticle/react-sidenav/issues"
  },
  "homepage": "https://github.com/codeparticle/react-sidenav#readme",
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4.1"
  },
  "dependencies": {
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "autoprefixer": "^10.4.2",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "chalk": "5.4.1",
    "classnames": "^2.3.1",
    "connected-react-router": "^6.9.1",
    "cssnano": "^5.0.6",
    "decode-uri-component": "^0.4.1",
    "dotenv": "10.0.0",
    "dotenv-expand": "5.1.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "4.0.2",
    "express": "^5.0.0-alpha.8",
    "file-loader": "6.2.0",
    "fs-extra": "10.0.0",
    "history": "^5.2.0",
    "html-webpack-plugin": "4.5.2",
    "identity-obj-proxy": "^3.0.0",
